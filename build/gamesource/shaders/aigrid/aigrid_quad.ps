#include <../struct.h>

//##########################################################################

sampler2D g_sColor:register(s0);

//##########################################################################

half4 main(in VSO_AIgrid IN):COLOR0
{
	return half4(lerp(tex2D(g_sColor,IN.vTexUV).xyz, IN.vColor.xyz, IN.vColor.a), 1.f);
}